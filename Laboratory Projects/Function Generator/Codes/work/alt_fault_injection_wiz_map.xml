<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="alt_fault_injection_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="FAULT INJECTION" WIZARD_NAME="FAULT INJECTION" MODULE_NAME="alt_fault_injection" >
			<PARAMETER NAME="IDC_CHECK_EMR_SHARE" CNX_TYPE="PRIVATE" TYPE="STRING" >
				<FORCE_VALUE TO="ENABLE_EMR_SHARE == NO ? 0 : 1" />
			</PARAMETER>
			<PARAMETER NAME="IDC_CHECK_INTOSC_SHARE" CNX_TYPE="PRIVATE" TYPE="STRING" >
				<FORCE_VALUE TO="ENABLE_INTOSC_SHARE == NO ? 0 : 1" />
			</PARAMETER>
			<PARAMETER NAME="IDC_EDCLK_DIVISOR" CNX_TYPE="PRIVATE" TYPE="INTEGER" VALUE="CRC_OSC_DIVIDER" />
			<PORT NAME="intosc" />
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
