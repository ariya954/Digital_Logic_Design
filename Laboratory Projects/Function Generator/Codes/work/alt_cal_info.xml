<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="altera_mf.altera_mf_components" />
		<HAS_SIM_LIB VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="CAL_BBPD_FIRST" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="CHANNEL_ADDRESS_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="ERROR_SIGNALS" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="NUMBER_OF_CHANNELS" TYPE="INTEGER" />
		<PARAMETER NAME="SIM_MODEL_MODE" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="WATCH_LENGTH" TYPE="INTEGER" DEFAULT_VALUE="100" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="alt_cal" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="busy" TYPE="OUTPUT" />
		<PORT NAME="cal_error" TYPE="OUTPUT" WIDTH="NUMBER_OF_CHANNELS" />
		<PORT NAME="clock" TYPE="INPUT" />
		<PORT NAME="dprio_addr" TYPE="OUTPUT" WIDTH="16" />
		<PORT NAME="dprio_busy" TYPE="INPUT" />
		<PORT NAME="dprio_datain" TYPE="INPUT" WIDTH="16" />
		<PORT NAME="dprio_dataout" TYPE="OUTPUT" WIDTH="16" />
		<PORT NAME="dprio_rden" TYPE="OUTPUT" />
		<PORT NAME="dprio_wren" TYPE="OUTPUT" />
		<PORT NAME="quad_addr" TYPE="OUTPUT" WIDTH="9" />
		<PORT NAME="remap_addr" TYPE="INPUT" WIDTH="12" DEFAULT_VALUE="0" />
		<PORT NAME="reset" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="retain_addr" TYPE="OUTPUT" />
		<PORT NAME="start" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="testbuses" TYPE="INPUT" WIDTH="NUMBER_OF_CHANNELS*4" DEFAULT_VALUE="0" />
		<PORT NAME="transceiver_init" TYPE="INPUT" CONTEXT="SYNTH_ONLY" />
	</PORTS>
</ROOT>
