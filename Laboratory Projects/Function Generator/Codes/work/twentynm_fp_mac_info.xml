		<PARAMETER NAME="adder_subtract" TYPE="String" VALUE="false|true" DEFAULT_VALUE="false" />
		<PARAMETER NAME="ax_clock" TYPE="String" VALUE="NONE|0|1|2" DEFAULT_VALUE="NONE" />
		<PARAMETER NAME="ay_clock" TYPE="String" VALUE="NONE|0|1|2" DEFAULT_VALUE="NONE" />
		<PARAMETER NAME="az_clock" TYPE="String" VALUE="NONE|0|1|2" DEFAULT_VALUE="NONE" />
		<PARAMETER NAME="output_clock" TYPE="String" VALUE="NONE|0|1|2" DEFAULT_VALUE="NONE" />
		<PARAMETER NAME="accumulate_clock" TYPE="String" VALUE="NONE|0|1|2" DEFAULT_VALUE="NONE" />
