<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="altera_mf.altera_mf_components" />
		<HAS_SIM_LIB VALUE="ON" />
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="ACF_DISABLE_MLAB_RAM_USE" TYPE="STRING" VALUE="false|true" DEFAULT_VALUE_INDEX="0" LPM_HINT="ON" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="NUMBER_OF_TAPS" TYPE="INTEGER" />
		<PARAMETER NAME="POWER_UP_STATE" TYPE="STRING" VALUE="CLEARED|DONT_CARE" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="RAM_BLOCK_TYPE" TYPE="STRING" VALUE="UNUSED" DEFAULT_VALUE_INDEX="0" LPM_HINT="ON" />
		<PARAMETER NAME="TAP_DISTANCE" TYPE="INTEGER" />
		<PARAMETER NAME="WIDTH" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altshift_taps" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="aclr" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="clken" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="clock" TYPE="INPUT" />
		<PORT NAME="shiftin" TYPE="INPUT" WIDTH="WIDTH" />
		<PORT NAME="shiftout" TYPE="OUTPUT" WIDTH="WIDTH" />
		<PORT NAME="taps" TYPE="OUTPUT" WIDTH="WIDTH*NUMBER_OF_TAPS" />
		<PORT NAME="sclr" TYPE="INPUT" DEFAULT_VALUE="0" CONTEXT="SIM_ONLY" />
	</PORTS>
</ROOT>
