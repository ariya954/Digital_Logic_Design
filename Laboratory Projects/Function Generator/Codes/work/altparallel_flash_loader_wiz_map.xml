		<PORT NAME="pfl_flash_access_request" CNX_TYPE="USED_PORT" />
		<PORT NAME="pfl_nreset" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_io0" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_io1" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_io2" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_io3" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_ncs" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_sck" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_ale" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_cle" CNX_TYPE="USED_PORT" />
		<PORT NAME="flash_io" CNX_TYPE="USED_PORT" />
    </WIZARD_MAP>
  </WIZARD_DATA>
</ROOT>
