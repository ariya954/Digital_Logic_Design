		<PARAMETER NAME="IDC_OPERATING_MODES_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" >
			<FORCE_VALUE TO="Flash Programming and FPGA Configuration" WHEN="FEATURES_PGM == 1 && FEATURES_CFG == 1" />
		</PARAMETER>
		<PARAMETER NAME="IDC_FLASH_TYPE_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="FLASH_TYPE" />
		<PARAMETER NAME="IDC_FLASH_DEVICE_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="CFI 8 Mbit" />
		<PARAMETER NAME="IDC_FLASH_DATA_WIDTH_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="FLASH_DATA_WIDTH" />
		<PARAMETER NAME="IDC_QFLASH_SIZE_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" />
		<PARAMETER NAME="IDC_QFLASH_MFC_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="QFLASH_MFC" />
		<PARAMETER NAME="IDC_NFLASH_MFC_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="NFLASH_MFC" />
