		<PARAMETER NAME="RSU_WATCHHDOG_CHECKBOX" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="0" />
		<PARAMETER NAME="CLOCK_FREQUENCY_EDIT" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="100.000" />
		<PARAMETER NAME="RSU_WATCHHDOG_COUNTER_EDIT" CNX_TYPE="PRIVATE" TYPE="STRING"  VALUE="100" />
		<PARAMETER NAME="IDC_NUM_FLASH_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="1" />
