<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="mfam_family_macros.xml" />
	<PARAMETERS>
		<PARAMETER NAME="WIZ_PROTOCOL" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="WIZ_SUBPROTOCOL" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="WIZ_INPUT_SELECTION" TYPE="INTEGER" VALUE="0" />		
	</PARAMETERS>
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALT4GXB" WIZARD_NAME="ALTGX" MODULE_NAME="alt4gxb" MAP_CONSTANTS="OFF">
			<PARAMETER NAME="WIZ_PROTOCOL" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="Basic"/>
				<FORCE_VALUE IF="PROTOCOL == gige" THEN="GIGE"/>
				<FORCE_VALUE IF="PROTOCOL == xaui" THEN="XAUI"/>
				<FORCE_VALUE IF="PROTOCOL == pcie || PROTOCOL == pcie2" THEN="'PCI Express (PIPE)'"/>
				<FORCE_VALUE IF="PROTOCOL == cei" THEN="'(OIF) CEI PHY Interface'"/>
				<FORCE_VALUE IF="PROTOCOL == sdi_3g || PROTOCOL == sdi_hd" THEN="SDI"/>
				<FORCE_VALUE IF="PROTOCOL == srio" THEN="'Serial Rapid IO'"/>								
				<FORCE_VALUE IF="PROTOCOL == sonet_oc12 || PROTOCOL == sonet_oc48 || PROTOCOL == sonet_oc96" THEN="'Sonet/SDH'"/>
				<FORCE_VALUE IF="PROTOCOL == cpri" THEN="'Deterministic Latency'" />
				<FORCE_VALUE IF="WIZ_PROTOCOL != UNUSED" THEN="WIZ_PROTOCOL" />
			</PARAMETER>
			<PARAMETER NAME="WIZ_SUBPROTOCOL" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="None"/>
				<FORCE_VALUE IF="PROTOCOL == sonet_oc96" THEN="OC96"/>
				<FORCE_VALUE IF="PROTOCOL == sonet_oc48" THEN="OC48"/>
				<FORCE_VALUE IF="(PROTOCOL == sonet_oc12)" THEN="OC12"/>
				<FORCE_VALUE IF="(PROTOCOL == pcie) and (RX_CHANNEL_BONDING == indv || TX_CHANNEL_BONDING == indv)" THEN="'Gen 1-x1'"/>
				<FORCE_VALUE IF="(PROTOCOL == pcie) and (RX_CHANNEL_BONDING == x4 || RX_CHANNEL_BONDING == x4_unbundled || TX_CHANNEL_BONDING == x4 || TX_CHANNEL_BONDING == x4_unbundled)" THEN="'Gen 1-x4'"/>
				<FORCE_VALUE IF="(PROTOCOL == pcie) and (RX_CHANNEL_BONDING == x8 || RX_CHANNEL_BONDING == x8_unbundled || TX_CHANNEL_BONDING == x8 || TX_CHANNEL_BONDING == x8_unbundled)" THEN="'Gen 1-x8'"/>
				<FORCE_VALUE IF="(PROTOCOL == pcie2) and (RX_CHANNEL_BONDING == indv || TX_CHANNEL_BONDING == indv)" THEN="'Gen 2-x1'"/>
				<FORCE_VALUE IF="(PROTOCOL == pcie2) and (RX_CHANNEL_BONDING == x4 || RX_CHANNEL_BONDING == x4_unbundled || TX_CHANNEL_BONDING == x4 || TX_CHANNEL_BONDING == x4_unbundled)" THEN="'Gen 2-x4'"/>
				<FORCE_VALUE IF="(PROTOCOL == pcie2) and (RX_CHANNEL_BONDING == x8 || RX_CHANNEL_BONDING == x8_unbundled || TX_CHANNEL_BONDING == x8 || TX_CHANNEL_BONDING == x8_unbundled)" THEN="'Gen 2-x8'"/>
				<FORCE_VALUE IF="PROTOCOL == sdi_3g" THEN="3G"/>
				<FORCE_VALUE IF="PROTOCOL == sdi_hd" THEN="HD"/>
				<FORCE_VALUE IF="(PROTOCOL == cpri) and (TX_CHANNEL_BONDING == X4)" THEN="X4" />
				<FORCE_VALUE IF="(PROTOCOL == basic) and (RX_CHANNEL_BONDING == x4 || TX_CHANNEL_BONDING == x4)" THEN="X4"/>
				<FORCE_VALUE IF="(PROTOCOL == basic) and (RX_CHANNEL_BONDING == x8 || TX_CHANNEL_BONDING == x8)" THEN="X8"/>
				<FORCE_VALUE IF="WIZ_SUBPROTOCOL != UNUSED" THEN="WIZ_SUBPROTOCOL" />
			</PARAMETER>
			<PARAMETER NAME="WIZ_INPUT_A" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE IF="WIZ_INPUT_SELECTION == 0" REGEXP_SOURCE="effective_data_rate" REGEXP_PATTERN="([0-9.]+) Mbps" REGEXP_SUBSTITUTION="$1" />
				<FORCE_VALUE IF="WIZ_INPUT_SELECTION == 1" REGEXP_SOURCE="input_clock_frequency" REGEXP_PATTERN="([0-9.]+) MHz" REGEXP_SUBSTITUTION="$1" />	
			</PARAMETER>
			<PARAMETER NAME="WIZ_INPUT_B" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE IF="WIZ_INPUT_SELECTION == 1" REGEXP_SOURCE="effective_data_rate" REGEXP_PATTERN="([0-9.]+) Mbps" REGEXP_SUBSTITUTION="$1" />
				<FORCE_VALUE IF="WIZ_INPUT_SELECTION == 0" REGEXP_SOURCE="input_clock_frequency" REGEXP_PATTERN="([0-9.]+) MHz" REGEXP_SUBSTITUTION="$1" />	
			</PARAMETER>
			<PARAMETER NAME="WIZ_ENABLE_EQUALIZER_CTRL" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE IF="((EQUALIZER_CTRL_A_SETTING == 0) and (EQUALIZER_CTRL_B_SETTING == 0) and (EQUALIZER_CTRL_C_SETTING == 0) and (EQUALIZER_CTRL_D_SETTING == 0) and (EQUALIZER_CTRL_V_SETTING == 0))" THEN="0" ELSE="1" />
			</PARAMETER>	
			<PARAMETER NAME="WIZ_EQUALIZER_CTRL_SETTING" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="((EQUALIZER_CTRL_D_SETTING == 7) ? 1 : 0) + ((EQUALIZER_CTRL_C_SETTING == 7) ? 3 : 0) + ((EQUALIZER_CTRL_B_SETTING == 7) ? 3 : 0) + ((EQUALIZER_CTRL_A_SETTING == 7) ? 4 : 0) + ((EQUALIZER_CTRL_V_SETTING gte 3) ? 1 : 0) + ((EQUALIZER_CTRL_V_SETTING gte 4) ? 1 : 0) + ((EQUALIZER_CTRL_V_SETTING gte 5) ? 1 : 0) + ((EQUALIZER_CTRL_V_SETTING gte 7) ? 1 : 0)" />
			</PARAMETER>
			<PARAMETER NAME="WIZ_RX_VCM" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE IF="RX_COMMON_MODE == 0.82v" THEN="0.82" ELSE="1.1" />
			</PARAMETER>
			<PARAMETER NAME="WIZ_VCCHTX" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="TX_ANALOG_POWER" />
			<PARAMETER NAME="WIZ_TX_VCM" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="0.7" />
				<FORCE_VALUE IF="TX_COMMON_MODE == 0.6v" THEN="0.6" />
				<FORCE_VALUE IF="TX_COMMON_MODE == 0.65v" THEN="0.65" />
			</PARAMETER>
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
