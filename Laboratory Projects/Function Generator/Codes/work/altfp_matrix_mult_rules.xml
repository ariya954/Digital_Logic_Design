<?xml version="1.0"?>
<ROOT>
	<PARAMETERS>
		<PARAMETER NAME="WIDTH_EXP" VALUE="8-11" >
			<VALUE_RULE>
				(WIDTH_EXP == 8) or (WIDTH_EXP == 11)
			</VALUE_RULE>
		</PARAMETER>
		<PARAMETER NAME="WIDTH_MAN" VALUE="23-52" >
			<VALUE_RULE>
				(((WIDTH_EXP == 8) and (WIDTH_MAN == 23)) or
				((WIDTH_EXP == 11) and (WIDTH_MAN == 52)))
			</VALUE_RULE>
		</PARAMETER>
		<PARAMETER NAME="BLOCKS" DEFAULT_VALUE="2" >
			<FORCE_RANGE MIN="2" MAX="48" />
			<VALUE_RULE>
				((BLOCKS % 2) == 0)
			</VALUE_RULE>
		</PARAMETER>
		<PARAMETER NAME="VECTORSIZE" DEFAULT_VALUE="8" >
			<FORCE_RANGE MIN="8" MAX="256" />
			<VALUE_RULE>
				((VECTORSIZE % (2*BLOCKS)) == 0)
			</VALUE_RULE>
			<VALUE_RULE>
				(((VECTORSIZE*VECTORSIZE)/BLOCKS) lte 2048)
			</VALUE_RULE>
			<VALUE_RULE WHEN="MODE == COMPLEX" >
				(VECTORSIZE/(2*BLOCKS) gte 2)
			</VALUE_RULE>
			<VALUE_RULE WHEN="MODE == NORMAL" >
				(VECTORSIZE/BLOCKS gte 2)
			</VALUE_RULE>
			<VALUE_RULE WHEN="MODE == COMPLEX" >
				(VECTORSIZE/(2*BLOCKS) lte 6)
			</VALUE_RULE>
			<VALUE_RULE WHEN="MODE == NORMAL" >
				(VECTORSIZE/BLOCKS lte 6)
			</VALUE_RULE>
		</PARAMETER>
		<PARAMETER NAME="ROWSAA" DEFAULT_VALUE="8" >
			<FORCE_RANGE MIN="2" MAX="256" />
		</PARAMETER>
		<PARAMETER NAME="COLUMNSAA" DEFAULT_VALUE="8" >
			<FORCE_RANGE MIN="8" MAX="256" />
			<VALUE_RULE>
				(((ROWSAA*COLUMNSAA)/BLOCKS) lte 2048)
			</VALUE_RULE>
			<VALUE_RULE>
				((COLUMNSAA % (2*BLOCKS)) == 0)
			</VALUE_RULE>
		</PARAMETER>
		<PARAMETER NAME="COLUMNSBB" DEFAULT_VALUE="8" >
			<FORCE_RANGE MIN="2" MAX="256" />
		</PARAMETER>
	</PARAMETERS>
	<PORTS>
		<PORT NAME="reset" TYPE="INPUT" USAGE="VARIABLE" DEFAULT_VALUE="0" />
		<PORT NAME="enable" TYPE="INPUT" USAGE="VARIABLE" DEFAULT_VALUE="1" />
		<PORT NAME="result" TYPE="OUTPUT" USAGE="UNUSED" />
	</PORTS>
</ROOT>
